{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "env": {
    "production": {
      "plugins": ["transform-react-remove-prop-types"]
    }
  },
  "plugins": [
    ["@babel/plugin-proposal-class-properties"],
    [
        "css-modules-transform",
        {
            "camelCase": true,
            "extensions": [ ".css", ".scss" ]
        }
    ],
    "dynamic-import-node"
]
}
